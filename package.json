{
  "name": "prompt-compression-contest",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "dist/api/index.js",
  "scripts": {
    "test": "mocha test/**/*.spec.mjs",
    "build:dataset": "pnpm run dataset:parse && pnpm run dataset:process",
    "dataset:parse": "node src/tools/prepare-dataset/commonsense_qa_parse.js && node src/tools/prepare-dataset/mmlu_parse.js",
    "dataset:process": "tsx src/tools/process.ts",
    "dev:api": "nodemon --exec 'node --loader ts-node/esm src/api/index.ts'",
    "typeorm": "NODE_ENV=development typeorm-ts-node-esm -d src/api/data-source.ts",
    "migration:generate": "pnpm run typeorm migration:generate ./src/api/migrations/migration",
    "migration:run": "pnpm run typeorm migration:run",
    "migration:revert": "pnpm run typeorm migration:revert",
    "lint": "eslint . --ext .ts,.js,.mjs",
    "lint:fix": "eslint . --ext .ts,.js,.mjs --fix",
    "format": "prettier --write \"**/*.{ts,js,mjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,mjs,json,md}\"",
    "check": "pnpm format:check && pnpm lint",
    "fix": "pnpm format && pnpm lint:fix",
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc --project tsconfig.json",
    "start": "tsx dist/api/index.js"
  },
  "keywords": [],
  "author": "klntsky",
  "license": "ISC",
  "dependencies": {
    "apache-arrow": "^19.0.1",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "connect-flash": "^0.1.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "fast-json-stable-stringify": "^2.1.0",
    "openai": "^4.96.2",
    "parquet-wasm": "^0.6.1",
    "parquetjs-lite": "^0.8.7",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "validator": "^13.15.15",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-flash": "^0.0.40",
    "@types/express": "^5.0.2",
    "@types/express-session": "^1.18.1",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.17.32",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.2",
    "@types/tough-cookie": "^4.0.5",
    "@types/validator": "^13.15.1",
    "@types/zxcvbn": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "axios-cookiejar-support": "^6.0.2",
    "chai": "^5.2.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "glob": "^11.0.2",
    "globals": "^16.2.0",
    "lodash": "^4.17.21",
    "mocha": "^11.5.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "tough-cookie": "^5.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
