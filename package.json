    "migration:generate": "pnpm run typeorm migration:generate ./src/api/migrations/migration",
    "migration:run": "pnpm run typeorm migration:run",
    "migration:revert": "pnpm run typeorm migration:revert",
    "lint": "eslint . --ext .ts,.js,.mjs",
    "lint:fix": "eslint . --ext .ts,.js,.mjs --fix",
    "format": "prettier --write \"**/*.{ts,js,mjs,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,mjs,json,md}\"",
    "check": "pnpm format:check && pnpm lint",
    "fix": "pnpm format && pnpm lint:fix"
